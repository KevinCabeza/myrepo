//multiplicar los numeros que van del uno al tres(se utiliza un bucle for porque se cuauntas veces se multiplica)
package estructurasdecontrol; 
import java.util.Scanner; 

public class bucle {

	public static void main(String[] args) {
// necesito un acumulador, un contador 
		Scanner entrada = new Scanner(System.in);
		int producto = 1;
		
		for (int i=1; 1<=3; i++)
			producto = producto*1;
	}
			
	
		
}
	
	